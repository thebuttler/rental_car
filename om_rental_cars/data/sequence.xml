<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for car model -->
        <record id="seq_carmodel" model="ir.sequence">
            <field name="name">Cars Sequence</field>
            <field name="code">car.model.sequence</field>
            <field name="prefix">CA</field>
            <field name="padding">3</field>
        </record>
    </data>
    <data noupdate="1">
        <!-- Sequence for clients model -->
        <record id="seq_clients" model="ir.sequence">
            <field name="name">Clients Sequence</field>
            <field name="code">client.details.sequence</field>
            <field name="prefix">CL</field>
            <field name="padding">3</field>
        </record>
    </data>
    <data noupdate="1">
        <!-- Sequence for contract model -->
        <record id="seq_contract" model="ir.sequence">
            <field name="name">Contract Sequence</field>
            <field name="code">rent.contract.sequence</field>
            <field name="prefix">XM</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>